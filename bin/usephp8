sudo a2dismod php5 php5.6 php7.0 php7.1 php7.2 php7.3 php7.4
sudo a2enmod php8 && sudo apache2ctl restart
sudo update-alternatives --set php /usr/bin/php8
sudo update-alternatives --set phar /usr/bin/phar8
sudo update-alternatives --set phar.phar /usr/bin/phar.phar8
sudo update-alternatives --set phpize /usr/bin/phpize8
sudo update-alternatives --set php-config /usr/bin/php-config8
php -v
